<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Sara Neidorf" %></title>
    <%= stylesheet_link_tag "lib/normalize.min.css" %>
    <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.7.2/css/all.min.css" %>
    <%= stylesheet_link_tag "site" %>
    <%= stylesheet_link_tag "site-links" %>
    <%= stylesheet_link_tag "social-links" %>
    <%= stylesheet_link_tag "header-splash" %>

  </head>
  <body>
    <div class="navbar page-column-wrapper">
      <div class="page-column">
        <div class="navbar-left">
          <%= partial 'partials/site-links' %>    
        </div>
        <div class="navbar-right">
          <%= partial 'partials/social-links' %>
        </div>
      </div>
    </div>
    <%= yield %>
  </body>
</html>
